{
  "expo": {
    "name": "C\u00e1mara Lepra",
    "slug": "tarea11-camara-lepra",
    "scheme": "tarea11",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
